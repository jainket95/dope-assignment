    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "json-server": "^1.0.0-beta.5",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.1.18",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",

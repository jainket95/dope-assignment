{
  "name": "ux-and-data-management",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx",
    "test": "jest --watch",
    "generate": "ts-node -P scripts/tsconfig.json scripts/generateTableData.ts && mv ./db.json ./public",
